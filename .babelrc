{
    "presets": ["babel-preset-expo"],
    "env": {
        "development": {
            "plugins": [ "transform-react-jsx-source" ]
        },
        "production": {
            "plugins": [ "transform-remove-console" ]
        }
    },
    "plugins": [
        ["module-resolver", {
            "alias": {
                "randombytes": "./randombytes",
                "crypto": "crypto-browserify"
            }
        }]
    ]
}
